:host {
  position: relative;
}

.list-bottom-view {
  padding: 30rpx 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  font-size: 10pt;
}
.list-bottom-view image {
  width: 40rpx; height: 40rpx;
  margin: 0 10rpx;
  background: #f8f8f8;
  border-radius: 50%;
  animation: playing 2s linear infinite;
  animation-play-state: running;
  -webkit-animation-play-state: running;
}